{
  "presets": [
    [
      '@babel/preset-env', { "debug": false }
    ],
    [
      "@babel/preset-react", { "development": false }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": true }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 2,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}
